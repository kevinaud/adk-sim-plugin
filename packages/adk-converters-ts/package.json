    "typescript": "~5.8.0",
    "vitest": "^3.0.0"
  },
  "keywords": [
    "adk",
    "simulator",
    "converters",
    "protobuf"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinaud/adk-sim-plugin.git",
    "directory": "packages/adk-converters-ts"
